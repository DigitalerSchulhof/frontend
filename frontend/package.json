{
  "name": "@dsh/frontend",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm:dev:*\"",
    "dev:pages": "ts-node -r source-map-support/register _next/custom/scripts/generate-pages --watch",
    "dev:pages:watch": "nodemon -w _next/custom/ -e ts --exec \"ts-node -r source-map-support/register _next/custom/scripts/generate-pages --watch\"",
    "dev:i18n": "ts-node -r source-map-support/register _next/custom/scripts/generate-i18n --watch",
    "dev:i18n:watch": "nodemon -w _next/custom/ -e ts --exec \"ts-node -r source-map-support/register _next/custom/scripts/generate-i18n --watch\"",
    "dev:graphql": "graphql-codegen --config _next/custom/codegen.ts --watch",
    "dev:env": "cp .env _next/.env #Todo",
    "dev:next": "cd _next && next dev",
    "build": "concurrently \"pnpm:build:*(!next|:watch)\" && pnpm run build:next",
    "build:loaders": "tsc -p _next/custom/loaders/tsconfig.json",
    "build:loaders:watch": "tsc -p _next/custom/loaders/tsconfig.json -w --preserveWatchOutput",
    "build:pages": "ts-node -r source-map-support/register _next/custom/scripts/generate-pages",
    "build:pages:watch": "nodemon -w _next/custom/ -e ts --exec \"ts-node -r source-map-support/register _next/custom/scripts/generate-pages\"",
    "build:i18n": "ts-node -r source-map-support/register _next/custom/scripts/generate-i18n",
    "build:i18n:watch": "nodemon -w _next/custom/ -e ts --exec \"ts-node -r source-map-support/register _next/custom/scripts/generate-i18n\"",
    "build:graphql": "graphql-codegen --config _next/custom/codegen.ts",
    "build:env": "cp .env _next/.env #Todo",
    "build:next": "cd _next && next build",
    "start": "cd _next && next start"
  },
  "dependencies": {
    "@next/font": "13.1.2",
    "eslint-config-next": "13.0.6",
    "intl-messageformat": "10.2.5",
    "js-cookie": "3.0.1",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "styled-components": "5.3.6",
    "urql": "3.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@formatjs/icu-messageformat-parser": "2.1.14",
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/typed-document-node": "2.3.12",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-operations": "2.5.12",
    "@graphql-typed-document-node/core": "3.1.1",
    "@types/js-cookie": "3.0.2",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/styled-components": "5.1.26",
    "@types/webpack": "5.28.0",
    "chokidar": "3.5.3",
    "concurrently": "7.6.0",
    "eslint": "8.29.0",
    "globby": "11.1.0",
    "graphql": "16.6.0",
    "json5": "2.2.3",
    "nodemon": "2.0.20",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "yaml": "2.1.3"
  }
}
