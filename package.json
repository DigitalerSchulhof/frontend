{
  "name": "@dsh/digitaler-schulhof",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "dev": "concurrently \"pnpm:dev:*\"",
    "dev:translation-map": "nodemon -w tools -e ts --exec \"ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register tools/translation-map --watch\"",
    "dev:translate-app-dir": "nodemon -w tools -e ts --exec \"ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register tools/translate-app-dir --watch\"",
    "dev:next": "next dev",
    "build": "concurrently \"pnpm:build:*(!next)\" && pnpm run build:next",
    "build:next": "next build",
    "build:translation-map": "ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register tools/translation-map",
    "build:translate-app-dir": "ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register tools/translate-app-dir",
    "start": "next start"
  },
  "dependencies": {
    "@formatjs/icu-messageformat-parser": "2.4.0",
    "@isaacs/ttlcache": "1.4.0",
    "arangojs": "8.3.0",
    "intl-messageformat": "10.3.5",
    "ioredis": "5.3.2",
    "js-cookie": "3.0.5",
    "jsonwebtoken": "9.0.0",
    "ms": "2.1.3",
    "next": "13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.1",
    "styled-components": "6.0.0-rc.1",
    "vality": "6.3.2",
    "vality-env": "1.0.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.58",
    "@swc/helpers": "0.5.1",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.1",
    "@types/js-cookie": "3.0.3",
    "@types/jsonwebtoken": "9.0.2",
    "@types/ms": "0.7.31",
    "@types/node": "18.16.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "chokidar": "3.5.3",
    "concurrently": "8.0.1",
    "csstype": "3.1.2",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "globby": "11.1.0",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "5.1.0-beta",
    "yaml": "2.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react-is"
      ]
    },
    "updateConfig": {
      "ignoreDependencies": [
        "globby",
        "@types/node"
      ]
    }
  }
}
