{
  "name": "@dsh/frontend",
  "version": "1.0.0",
  "scripts": {
    "generate:types": "concurrently \"pnpm run generate:types:graphql\" \"pnpm run generate:types:t\"",
    "generate:types:graphql": "graphql-codegen --config codegen.ts",
    "generate:types:t": "node -r source-map-support/register .custom/scripts/generate-t",
    "dev": "NODE_ENV=development next dev",
    "build": "pnpm run build:custom && pnpm run build:pages && next build",
    "build:watch": "concurrently \"pnpm run build:custom:watch\" \"pnpm run build:pages:watch\"",
    "build:custom": "rm -rf .custom && tsc --build custom/tsconfig.json",
    "build:custom:watch": "tsc --build custom/tsconfig.json -w",
    "build:pages": "node -r source-map-support/register .custom/scripts/generate-pages",
    "build:pages:watch": "nodemon --exec \"node -r source-map-support/register .custom/scripts/generate-pages\"",
    "start": "next start"
  },
  "dependencies": {
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/typed-document-node": "2.3.12",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-operations": "2.5.12",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/webpack": "5.28.0",
    "concurrently": "7.6.0",
    "eslint": "8.29.0",
    "globby": "11.1.0",
    "graphql": "16.6.0",
    "json5": "2.2.3",
    "nodemon": "2.0.20",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "4.9.3",
    "yaml": "2.1.3"
  }
}
