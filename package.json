{
  "name": "@dsh/frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "dev": "concurrently \"pnpm:dev:*\"",
    "dev:translation-map": "nodemon -w tools -e ts --exec \"ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register tools/translation-map --watch\"",
    "dev:translate-app-dir": "nodemon -w tools -e ts --exec \"ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register tools/translate-app-dir --watch\"",
    "dev:next": "next dev",
    "build": "concurrently \"pnpm:build:*(!next)\" && pnpm run build:next",
    "build:next": "next build",
    "build:translation-map": "ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register tools/translation-map",
    "build:translate-app-dir": "ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register tools/translate-app-dir",
    "start": "next start"
  },
  "engines": {
    "node": "18",
    "pnpm": "8"
  },
  "dependencies": {
    "@dsh/protocols": "link:../protocols/clients/ts/dist",
    "@formatjs/icu-messageformat-parser": "2.6.0",
    "@grpc/grpc-js": "1.8.18",
    "@isaacs/ttlcache": "1.4.1",
    "arangojs": "8.4.0",
    "intl-messageformat": "10.5.0",
    "ioredis": "5.3.2",
    "js-cookie": "3.0.5",
    "jsonwebtoken": "9.0.1",
    "ms": "2.1.3",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.4",
    "styled-components": "6.0.5",
    "vality": "6.3.3",
    "vality-env": "1.0.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.70",
    "@swc/helpers": "0.5.1",
    "@swc/jest": "0.2.27",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/jest": "29.5.3",
    "@types/js-cookie": "3.0.3",
    "@types/jsonwebtoken": "9.0.2",
    "@types/ms": "0.7.31",
    "@types/node": "18.16.3",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@welldone-software/why-did-you-render": "7.0.1",
    "chokidar": "3.5.3",
    "concurrently": "8.2.0",
    "csstype": "3.1.2",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "globby": "11.1.0",
    "jest": "29.6.1",
    "nodemon": "3.0.1",
    "prettier": "3.0.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6",
    "yaml": "2.3.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react-is"
      ]
    },
    "updateConfig": {
      "ignoreDependencies": [
        "globby",
        "@types/node"
      ]
    }
  }
}
