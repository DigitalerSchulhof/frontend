{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "incremental": true,
    "module": "CommonJS",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitOverride": true
  }
}
